{
  "name": "weather_api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npx dotenv -e .env.prod --  nest start",
    "start:dev": "npx dotenv -e .env.dev -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "dotenv -e .env.prod -- node dist/src/main",
    "migrate:dev": "npx dotenv -e .env.dev -- npx prisma migrate deploy",
    "migrate:create": "npx dotenv -e .env.dev -- npx prisma migrate dev --create-only",
    "migrate:dev:apply": "npx dotenv -e .env.dev -- npx prisma migrate dev",
    "migrate:reset": "npx dotenv -e .env.dev -- npx prisma migrate reset",
    "migrate:prod": "npx dotenv -e .env.prod -- npx prisma migrate deploy",
    "seed:dev": "npx dotenv -e .env.dev -- npx prisma db seed",
    "studio:dev": "npx dotenv -e .env.dev -- npx prisma studio",
    "studio:prod": "npx dotenv -e .env.prod -- npx prisma studio",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.1.1",
    "@nestjs/core": "^11.1.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.1.1",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "6.8.2",
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "nodemailer": "^7.0.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "slugify": "^1.6.6",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.1",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "22.15.18",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.8.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed/seed.ts"
  },
  "_moduleAliases": {
    "@app": "./dist",
    "@common": "dist/src/common",
    "@config": "dist/src/config",
    "@core": "dist/src/core",
    "@modules": "dist/src/modules",
    "@prisma": "dist/prisma",
    "@test": "dist/test"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "trustedDependencies": [
    "@nestjs/core",
    "@prisma/client",
    "@prisma/engines",
    "@scarf/scarf",
    "prisma"
  ]
}
